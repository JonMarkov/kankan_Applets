page{
  background: #fff
}
.header {
  margin: 90rpx 0 90rpx 50rpx;
  border-bottom: 1px solid #ccc;
  text-align: center;
  width: 650rpx;
  height: 300rpx;
  line-height: 450rpx;
}

.header image {
  width: 200rpx;
  height: 200rpx;
  border-radius: 20rpx
}

.content {
  margin-left: 50rpx;
  margin-bottom: 90rpx;
}

.content text {
  display: block;
  color: #9d9d9d;
  margin-top: 0rpx;
  font-size: 26rpx
}

.bottom {
  background: #4AC268;
  border-radius: 10rpx;
  margin: 70rpx 50rpx;
  font-size: 26rpx;
  font-weight:bold;
  padding: 10rpx;
}
.title_sq{
  font-size: 40rpx
}